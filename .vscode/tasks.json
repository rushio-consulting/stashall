  "version": "2.0.0",
  "tasks": [
    {
      "label": "generate code from protos",
      "type": "shell",
      "command": "protoc",
      "args": [
        "--dart_out=grpc:common/generated_io/lib/src",
        "-Iprotos",
        "-Ithird_party/protobuf/src",
        "-Ithird_party/googleapis",
        "google/protobuf/empty.proto",
        "services/authentication_service.proto"
      ],
    }
  ]
}